            <div class="input-field col s12">
              <input placeholder="" id="first_name" type="text" class="validate">
              <label for="BWType">Type:</label>
            </div>
          </div>
          <div class="row">
            <div class="input-field col s12">
              <input value="" id="disabled" type="text" class="validate">
